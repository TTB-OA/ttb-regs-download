Table titles {
  title_number int [pk, note: 'the title number of the eCFR, e.g., 27 for Title 27']
  title_label varchar
  latest_issue_date date
  up_to_date_as_of date
  reserved boolean
  title_details_download_date datetime [note: 'the date the title details / structure was last downloaded from the eCFR']
}

Table title_details {
  cfr_ref varchar [pk note: 'the CFR reference, generated from the components of the item']
  reg_text text [note: 'the text of the regulation, as it appears in the eCFR']
  reg_text_download_date datetime [note: 'the date the regulation text was last downloaded from the eCFR']
  hierarchy_type varchar [note: 'the type of hierarchy level, e.g., "chapter", "subchapter", "part", "subpart", "section", "appendix"']
  hierarchy_level int [note: 'the level of the hierarchy, starting from 1 for the highest level']
  is_leaf_node boolean [note: 'true if this item has no children (is a terminal/leaf node in the hierarchy), false if it has child elements']
  reserved boolean [note: 'true if this hierarchy element is reserved (not currently in use), false if it contains active regulations']
  order_id int [note: 'the order of the item within its hierarchy level, used for sorting']
  title_number int [ref: > titles.title_number]
  chapter_id varchar
  chapter_label varchar
  subchapter_id varchar
  subchapter_label varchar
  part_id varchar
  part_label varchar
  subpart_id varchar
  subpart_label varchar
  section_id varchar
  section_label varchar
  appendix_id varchar
  appendix_label varchar
  subject_grp_id varchar
  subject_grp_label varchar
}